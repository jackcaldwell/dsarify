{
  "name": "dsarify",
  "version": "2.0.0",
  "description": "DSAR PST Processing Pipeline for UK GDPR compliance with AI-powered redaction",
  "license": "ISC",
  "author": "",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "parse": "node src/parse.js",
    "filter": "node src/filter-messages.js",
    "filter:senders": "node src/filter-by-sender.js",
    "dedup": "node src/dedup-messages.js",
    "dedup:by-sender": "DEDUP_INPUT=output/filtered-messages-by-sender.json DEDUP_OUTPUT=output/filtered-messages-by-sender-deduped.json node src/dedup-messages.js",
    "redact": "node src/redact-v2.js",
    "redact:filtered": "REDACT_INPUT=output/filtered-messages.json node src/redact-v2.js",
    "redact:by-sender": "REDACT_INPUT=output/filtered-messages-by-sender.json node src/redact-v2.js",
    "redact:by-sender-deduped": "REDACT_INPUT=output/filtered-messages-by-sender-deduped.json node src/redact-v2.js",
    "redact:basic": "node src/redact.js",
    "unredact:subject": "node src/unredact-subject.js",
    "pdf": "node src/generate-pdf.js",
    "pdf:filtered": "PDF_INPUT=output/redacted-filtered-messages.json node src/generate-pdf.js",
    "pdf:deduped": "PDF_INPUT=output/redacted-filtered-messages-deduped.json node src/generate-pdf.js",
    "pdf:raw": "node src/generate-pdf-raw.js",
    "preview": "node src/generate-preview-pdf.js",
    "clean": "rm -rf output/*",
    "reset-checkpoint": "rm -f output/redaction-checkpoint.json",
    "demo": "node parse-pst.js"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "html-to-text": "^9.0.5",
    "openai": "^6.16.0",
    "pdfkit": "^0.17.2",
    "pst-extractor": "^1.12.0"
  }
}
